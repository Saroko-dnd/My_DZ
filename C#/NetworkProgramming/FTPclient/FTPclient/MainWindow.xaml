<Window x:Class="FTPclient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:MyRes="clr-namespace:FTPclient.MyResourses"
        xmlns:local="clr-namespace:FTPclient"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="325"  ResizeMode="NoResize">
    <StackPanel>
        <Button Name="StartFtpRequestButton" Content="{x:Static MyRes:Texts.StartGame}" Click="StartFtpRequestButton_Click"></Button>
        <TextBox Height="250" MaxLines="10"  IsReadOnly="True" Name="GameFieldTextBox"></TextBox>
        <WrapPanel>
            <Label Content="{x:Static MyRes:Texts.Choice}"></Label>
            <ComboBox Name="ChooseSymbolComboBox"></ComboBox>
        </WrapPanel>
    </StackPanel>
</Window>
