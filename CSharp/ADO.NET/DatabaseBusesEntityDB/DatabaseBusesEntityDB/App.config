<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
        <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <connectionStrings>
    <!--Не указываем метаданные т.к. не используется EDMX (файлы XML), в качестве провайдера указываем System.Data.SqlClient-->
    <add name="TransportConnectionString" connectionString="data source=(LocalDb)\v11.0;initial catalog=TransportDB;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework" providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>