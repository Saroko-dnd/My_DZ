
@using NewsWebsite.ClassesForNewsWebsite
@using NewsInfrastructure
@using NewsWebsite.Areas.News.Models
@model IEnumerable<News>

@{
    Layout = null;
}

@foreach (News CurrentNews in Model)
{
    string CurrentDivId = "DivForNews_" + CurrentNews.NewsID.ToString();
    <div class="FlexColumnCenter" id="@CurrentDivId">
        @Html.Partial(ApplicationConstants.PathFromRouteToNewsPartialView, new NewsForPartialView(CurrentNews))
    </div>
}